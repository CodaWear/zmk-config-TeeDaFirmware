#include "TeeDa-layouts.dtsi"
#include <dt-bindings/zmk/matrix_transform.h>
/ {
    chosen {
        zmk,physical-layout = &TeeDa_physical_layout;
    };
};
/ {
    kscan0: kscan0 {
        compatible = "zmk,kscan-gpio-matrix";
        diode-direction = "col2row";
        wakeup-source;

        row-gpios
            = <&pro_micro 21 (GPIO_ACTIVE_HIGH | GPIO_PULL_DOWN)>
            , <&pro_micro 20 (GPIO_ACTIVE_HIGH | GPIO_PULL_DOWN)>
            , <&pro_micro 19 (GPIO_ACTIVE_HIGH | GPIO_PULL_DOWN)>
            , <&pro_micro 18 (GPIO_ACTIVE_HIGH | GPIO_PULL_DOWN)>
            , <&pro_micro 15 (GPIO_ACTIVE_HIGH | GPIO_PULL_DOWN)>
            ;
    };
    default_transform: keymap_transform0 {
        compatible = "zmk,matrix-transform";
        columns = <12>;
        rows = <5>;
        map = <
        //         |LKey 1 |LKey 2 |LKey 3 |LKey 4 |LKey 5         RKey 1 |RKey 2 |RKey 3 |RKey 4 |RKey 5 |
                    RC(1,0) RC(2,0) RC(3,0) RC(4,0) RC(5,0)        RC(6,0) RC(7,0) RC(8,0) RC(9,0) RC(10,0)
        //  LKey 6 |LKey 7 |LKey 8 |LKey 9 |LKey 10 |LKey 11       RKey 6 |RKey 7 |RKey 8 |RKey 9 |RKey 10 |RKey 11
            RC(0,1) RC(1,1) RC(2,1) RC(3,1) RC(4,1) RC(5,1)        RC(6,1) RC(7,1) RC(8,1) RC(9,1) RC(10,1) RC(11,1)
        //  LKey 12 |LKey 13 |LKey 14 |LKey 15 |LKey 16 |LKey 17   RKey 12 |RKey 13 |RKey 14 |RKey 15 |RKey 16 |RKey 17
            RC(0,2) RC(1,2) RC(2,2) RC(3,2) RC(4,2) RC(5,2)        RC(6,2) RC(7,2) RC(8,2) RC(9,2) RC(10,2) RC(11,2)
        //  LKey 18 |LKey 19 |LKey 20 |LKey 21 |LKey 22 |LKey 23   RKey 18 |RKey 19 |RKey 20 |RKey 21 |RKey 22 |RKey 23
            RC(0,3) RC(1,3) RC(2,3) RC(3,3) RC(4,3) RC(5,3)        RC(6,3) RC(7,3) RC(8,3) RC(9,3) RC(10,3) RC(11,3)
        //                            |LKey 24 |LKey 25 |LKey 26   RKey 24 |RKey 25 |RKey 26|
                                       RC(3,4) RC(4,4) RC(5,4)     RC(6,4) RC(7,4) RC(8,4)
        >;
    };
};